{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "fmt": "npx @biomejs/biome check --write ./src vite.config.ts tsconfig.json tsconfig.node.json README.md eslint.config.js package.json package-lock.json index.html",
    "visualizer": "vite-bundle-visualizer",
    "lint": "eslint src/**/*.{ts,tsx} --report-unused-disable-directives --max-warnings 0",
    "pre-commit": "npm run fmt && npm run lint",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "framer-motion": "^11.2.12",
    "markdown-to-jsx": "^7.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.2",
    "@eslint/js": "^9.5.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "vite": "^5.3.1",
    "vite-bundle-visualizer": "^1.2.1"
  }
}
