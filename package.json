{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "npm run pre-commit && npm run build",
    "build": "tsc && vite build",
    "fmt": "npx @biomejs/biome check --write --changed",
    "fmt-all": "npx @biomejs/biome check --write",
    "fix": "npx @biomejs/biome check --write --unsafe --changed",
    "fix-all": "npx @biomejs/biome check --write --unsafe",
    "visualizer": "vite-bundle-visualizer",
    "pre-commit": "npm run fmt-all",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/lab": "^6.0.0-beta.10",
    "@mui/material": "^6.1.1",
    "framer-motion": "^11.9.0",
    "markdown-to-jsx": "^7.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "swiper": "^11.1.14"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@types/node": "^22.7.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "typescript": "^5.6.2",
    "vite": "^6.0.3",
    "vite-bundle-visualizer": "^1.2.1"
  }
}
